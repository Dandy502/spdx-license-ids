{
  "name": "spdx-license-identifiers",
  "version": "1.1.0",
  "description": "A list of SPDX license identifiers",
  "repository": "shinnn/spdx-license-identifiers",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "build": "node build.js",
    "pretest": "${npm_package_scripts_build} && eslint *.js && jscs *.js",
    "test": "node test.js",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/spdx-license-identifiers/blob/master/LICENSE"
    }
  ],
  "main": "spdx-license-identifiers-cjs.js",
  "files": [
    "spdx-license-identifiers-cjs.js"
  ],
  "keywords": [
    "spdx",
    "license",
    "licenses",
    "id",
    "identifier",
    "identifiers",
    "json",
    "array",
    "oss",
    "browser",
    "client-side"
  ],
  "devDependencies": {
    "each-async": "^1.1.1",
    "eslint": "^0.15.1",
    "istanbul": "^0.3.6",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.11.3",
    "require-bower-files": "^2.0.0",
    "rm-rf": "^0.1.0",
    "spdx-license-list": "^1.1.0",
    "stringify-object": "^1.0.0",
    "tape": "^3.5.0",
    "tiny-npm-license": "^1.0.0"
  },
  "jscsConfig": {
    "preset": "google",
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
